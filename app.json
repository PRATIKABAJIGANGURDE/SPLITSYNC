{
  "expo": {
    "name": "SplitSync",
    "slug": "splitsync",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpeg",
    "scheme": "splitsync-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitsync.app"
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.splitsync.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://splitsync.app/"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.20",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24
          }
        }
      ],
      "expo-font",
      "@react-native-community/datetimepicker",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://splitsync.app/"
      },
      "eas": {
        "projectId": "fbef358a-1923-4a18-b59b-e6ae50f2aaa5"
      }
    }
  }
}
